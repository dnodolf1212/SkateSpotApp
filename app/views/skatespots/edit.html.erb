<h1>Edit <%= @skatespot.name %></h1>
<%= form_for @skatespot do |f| %> 
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :category %><br>
  <%= f.text_field :category %><br>
  <%= f.label :description %><br>
  <%= f.text_field :description %><br>
  <%= f.submit %>
  <% end %>